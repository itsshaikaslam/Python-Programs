#Write a program to find GCD of two numbers
def getGCD(num1,num2):
	while num1!=num2:
		if(num1>num2):
			num1=num1-num2
		else:
			num2=num2-num1
	return num1
def main():
	num1=input("Enter 1st number :")
	num2=input("Enter 2nd number :")
	result=getGCD(num1,num2)
	print("GCD of {}, {} is {}".format(num1,num2,result))
if __name__=="__main__":
	main()
